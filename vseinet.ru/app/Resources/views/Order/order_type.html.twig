<fieldset class="block">
    <div class="wrapper">
        <h2>Заказ на</h2>
        {% for choice in form.typeCode.vars.choices %}
        <div class="row v">
            <label for="{{ form.typeCode.vars.id }}_{{ choice.value }}">
                {{ choice.label }}
                <input type="radio" id="{{ form.typeCode.vars.id }}_{{ choice.value }}" name="{{ form.typeCode.vars.full_name }}" value="{{ choice.value }}" required="required"{% if (choice.value == form.typeCode.vars.value)  %} checked="checked"{% endif %} />
            </label>
        </div>
        {% endfor %}
    </div>
</fieldset>
